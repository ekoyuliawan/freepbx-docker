[asterisk]
ignoreip    = 127.0.0.1/8 ::1
              10.0.0.0/8
              172.20.0.0/12
              192.168.0.0/16
              192.0.0.0/16
enabled = true
backend = auto
filter = asterisk
logpath = /var/log/asterisk/full
maxretry = 3
findtime = 30
bantime = 1d
action = iptables-allports[protocol=all]
[sshd]
enabled = false
